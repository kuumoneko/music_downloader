{
  "name": "downloader",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "run": "npm --loader ts-node/esm index.ts",
    "build test exe": "deno compile --no-check --allow-env --allow-read  --allow-net --allow-write --allow-run ./test/lmao.ts",
    "build index exe": "deno compile --no-check --allow-env --allow-read --allow-net --allow-write --allow-run ./index.ts",
    "build dist exe": "deno compile --no-check --allow-env --allow-read --allow-net --allow-write --allow-run --output downloader ./downloader/index.ts",
    "index": "node --trace-warnings --trace-deprecation --loader ts-node/esm  \"e:\\downloader\\index.ts\"",
    "test": "node --trace-warnings --trace-deprecation --loader ts-node/esm  \"e:\\downloader\\downloader\\src\\install_ffmpeg.ts\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/yargs": "^17.0.33",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "inspector": "^0.5.0",
    "yargs": "^17.7.2"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "exe"
  },
  "bin": "./index.ts"
}
